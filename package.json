{
  "name": "the-brain-ai-scraper",
  "version": "0.1.0",
  "description": "Intelligent Web Scraping API using Fastify, TypeScript, and LLMs",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run build && concurrently \"npm run build:watch\" \"nodemon dist/server.js\"",
    "build": "prisma generate && tsc && cpx 'src/generated/prisma/**/*' dist/generated/prisma && cpx prisma/schema.prisma dist/prisma && cpx .env dist",
    "build:watch": "tsc --watch",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest run"
  },
  "keywords": [
    "web scraping",
    "api",
    "fastify",
    "typescript",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^6.3.0",
    "@fastify/sensible": "^6.0.3",
    "@nestjs/common": "^11.0.19",
    "@nestjs/config": "^4.0.2",
    "@prisma/client": "^6.6.0",
    "@sinclair/typebox": "^0.34.33",
    "@types/cheerio": "^0.22.35",
    "cheerio": "^1.0.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.0",
    "openai": "^4.94.0",
    "playwright": "^1.51.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "fastify-plugin": "^5.0.1",
    "nodemon": "^3.1.9",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vitest": "^3.1.1"
  }
}

{
  "name": "the-brain-ai-scraper",
  "version": "0.1.0",
  "description": "Intelligent Web Scraping API using Fastify, TypeScript, and LLMs",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npm run build && concurrently \"npm run build:watch\" \"nodemon dist/server.js\"",
    "build": "prisma generate && tsc && cpx 'src/generated/prisma/**/*' dist/generated/prisma && cpx prisma/schema.prisma dist/prisma && cpx .env dist",
    "build:watch": "tsc --watch",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:with-db": "npm run setup:test-db && DATABASE_URL_TEST=postgresql://postgres:postgres@localhost:5432/postgres vitest run",
    "setup:test-db": "ts-node-esm tests/utils/setup-test-db.ts"
  },
  "keywords": [
    "web scraping",
    "api",
    "fastify",
    "typescript",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^6.3.0",
    "@fastify/sensible": "^6.0.3",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@nestjs/common": "^11.0.20",
    "@nestjs/config": "^4.0.2",
    "@prisma/client": "^6.6.0",
    "@sinclair/typebox": "^0.34.33",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.8.4",
    "braces": "^3.0.3",
    "cheerio": "^1.0.0",
    "chokidar": "^3.6.0",
    "core-js": "^3.41.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.0",
    "glob": "^9.3.5",
    "lru-cache": "^11.1.0",
    "openai": "^4.95.0",
    "playwright": "^1.51.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/axios": "^0.14.4",
    "@types/eventsource": "^3.0.0",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitest/utils": "^3.1.1",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "eslint": "^9.24.0",
    "fastify-plugin": "^5.0.1",
    "nodemon": "^3.1.9",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
}
